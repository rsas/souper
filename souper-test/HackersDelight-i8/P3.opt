; REQUIRES: solver, solver-model, synthesis

; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-ignore-cost -souper-synthesis-comps=const,and,sub %s > %t1
; RUN: FileCheck %s < %t1

; CHECK: result %2

; Isolate the right-most 1-bit
%0:i8 = var
%1:i8 = sub 0, %0
%2:i8 = and %0, %1
infer %2
