; REQUIRES: solver, solver-model, synthesis

; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-ignore-cost -souper-synthesis-comps=ule,and,xor,const %s > %t1
; RUN: FileCheck %s < %t1

; CHECK:      %5:i8 = {{(and|xor)}} %0, %1
; CHECK-NEXT: %6:i8 = {{(xor|and)}} %0, %5
; CHECK-NEXT: %7:i1 = ule %6, %1
; CHECK-NEXT: result %7

; Test if nlz(x) <= nlz(y) where nlz is number of leading zeros
%0:i8 = var
%1:i8 = var
%2:i8 = xor %1, -1
%3:i8 = and %0, %2
%4:i1 = ule %3, %1
infer %4
