; REQUIRES: solver, solver-model, synthesis

; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-ignore-cost -souper-synthesis-comps=ule,and,xor %s > %t1
; RUN: FileCheck %s < %t1

; CHECK: result %4

; Test if nlz(x) == nlz(y) where nlz is number of leading zeros
%0:i64 = var
%1:i64 = var
%2:i64 = and %0, %1
%3:i64 = xor %0, %1
%4:i1 = ule %3, %2
infer %4
