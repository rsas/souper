; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-comps=const,eq -souper-exploit-ub=false -souper-synthesis-debug -souper-synthesis-debug-level=2 %s > %t1
; RUN: FileCheck %s < %t1

; CHECK: cand reached MaxWiringAttempts
; CHECK: Failed to infer RHS

%0 = block 2
%1:i32 = var
%2:i32 = add 4294967295:i32, %1
%3:i16 = trunc %2
%4:i16 = lshr 32907:i16, %3
%5:i16 = and 1:i16, %4
%6:i1 = ne 0:i16, %5
%7:i1 = phi %0, %6, 0:i1
infer %7
